<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yunhe.doit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".A_MainActivity"
            android:label="@string/title_activity_a_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <!-- 注册自定义静态广播接收器 -->  
        <receiver android:name="yunhe.doit.receiver.LinkOkReceiver">  
            <intent-filter>  
                <action android:name="yunhe.doit.urlLink" />  
            </intent-filter>  
        </receiver>  
        
        <activity android:name="B_EditContentActivity"></activity>
        <activity android:name="C_InfoActivity"></activity>
        <activity android:name="B_ListContentActivity"></activity>
        <activity android:name="D_TestFunctionActivity"></activity>
        <activity android:name="yunhe.doit.testFunction.D_1_SFATestActivity"></activity>
    </application>

</manifest>